@{
    Layout = null;
}
<!doctype html>
<html class="no-js" lang="en">
<head>
    <meta charset="utf-8">

    <!--====== Title ======-->
    <title>PocketMenu - Menünüz Cepte!</title>

    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!--====== Favicon Icon ======-->
    <link rel="shortcut icon" href="~/Content/src/landing-assets/images/favicon.png" type="image/png">

    <!--====== Animate CSS ======-->
    <link rel="stylesheet" href="~/Content/src/landing-assets/css/animate.css">

    <!--====== Line Icons CSS ======-->
    <link rel="stylesheet" href="~/Content/src/landing-assets/css/LineIcons.2.0.css">

    <!--====== Bootstrap CSS ======-->
    <link rel="stylesheet" href="~/Content/src/landing-assets/css/bootstrap-4.5.0.min.css">

    <!--====== Default CSS ======-->
    <link rel="stylesheet" href="~/Content/src/landing-assets/css/default.css">

    <!--====== Style CSS ======-->
    <link rel="stylesheet" href="~/Content/src/landing-assets/css/style.css">

</head>

<body>
    <!--[if IE]>
      <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="https://browsehappy.com/">upgrade your browser</a> to improve your experience and security.</p>
    <![endif]-->
    <!--====== PRELOADER PART START ======-->

    <div class="preloader">
        <div class="loader">
            <div class="ytp-spinner">
                <div class="ytp-spinner-container">
                    <div class="ytp-spinner-rotator">
                        <div class="ytp-spinner-left">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                        <div class="ytp-spinner-right">
                            <div class="ytp-spinner-circle"></div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>

    <!--====== PRELOADER PART ENDS ======-->
    <!--====== HEADER PART START ======-->

    <header class="header-area">
        <div class="navbar-area">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        <nav class="navbar navbar-expand-lg">
                            <a class="navbar-brand" href="@Url.Action("Landing","Home")">
                                <img src="~/Content/src/landing-assets/images/logo.svg" height="45" alt="Logo">
                            </a>
                            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                                <span class="toggler-icon"></span>
                            </button>

                            <div class="collapse navbar-collapse sub-menu-bar" id="navbarSupportedContent">
                                <ul id="nav" class="navbar-nav ml-auto">
                                    <li class="nav-item active">
                                        <a class="page-scroll" href="#home">PocketMenu</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="page-scroll" href="#features">Nasıl?</a>
                                    </li>
                                    <li class="nav-item">
                                        <a class="page-scroll" href="#about">Nedir?</a>
                                    </li>
                                    @*<li class="nav-item">
                                            <a class="page-scroll" href="#facts">Neden?</a>
                                        </li>*@
                                    <li class="nav-item">
                                        <a class="page-scroll" href="#contact">İletişim</a>
                                    </li>
                                </ul>
                            </div> <!-- navbar collapse -->

                            <div class="navbar-btn d-none d-sm-inline-block">
                                <a class="main-btn" data-scroll-nav="0" href="@Url.Action("In", "Sign")" rel="nofollow">Giriş Yap!</a>
                            </div>
                        </nav> <!-- navbar -->
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
        </div> <!-- navbar area -->

        <div id="home" class="header-hero bg_cover" style="background-image: url(/Content/src/landing-assets/images/banner-bg.svg)">
            <div class="container">
                <div class="row justify-content-center">
                    <div class="col-lg-8">
                        <div class="header-hero-content text-center">
                            @*<h3 class="header-sub-title wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="0.2s">404</h3>*@
                            <h2 class="header-title wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="0.5s">Aradığın Sayfa Bulunamadı</h2>
                            <p class="text wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="0.8s">Böyle bir sayfa yok. Belki de var ama bilmeni istemiyoruz.</p>
                            <a href="@Url.Action("Landing", "Home")" class="main-btn wow fadeInUp" data-wow-duration="1.3s" data-wow-delay="1.1s">Geri Dön</a>
                        </div> <!-- header hero content -->
                    </div>
                </div> <!-- row -->
                <div class="row">
                    <div class="col-lg-12">
                        <div class="header-hero-image text-center wow fadeIn" data-wow-duration="1.3s" data-wow-delay="1.4s">
                            <img src="~/Content/src/landing-assets/images/404.svg" alt="hero">
                        </div> <!-- header hero image -->
                    </div>
                </div> <!-- row -->
            </div> <!-- container -->
            <div id="particles-1" class="particles"></div>
        </div> <!-- header hero -->
    </header>

    <!--====== VIDEO COUNTER PART ENDS ======-->
    <!--====== FOOTER PART START ======-->

    <footer id="footer" class="footer-area pt-120">
        <div class="container">
            <div class="subscribe-area wow fadeIn" data-wow-duration="1s" data-wow-delay="0.5s">
                <div class="row">
                    <div class="col-lg-6">
                        <div class="subscribe-content mt-45">
                            <h2 class="subscribe-title">Güncellemelerimizi <span>takip etmek ister misiniz?</span></h2>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="subscribe-form mt-50">
                            <form action="#">
                                <input type="text" placeholder="örnek@örnek.com">
                                <button class="main-btn">Abone Ol</button>
                            </form>
                        </div>
                    </div>
                </div> <!-- row -->
            </div> <!-- subscribe area -->
            <div class="footer-widget pb-100">
                <div class="row">
                    <div class="col-lg-4 col-md-6 col-sm-8">
                        <div class="footer-about mt-50 wow fadeIn" data-wow-duration="1s" data-wow-delay="0.2s">
                            <a class="logo" href="#">
                                <img src="~/Content/src/landing-assets/images/logo.svg" alt="logo">
                            </a>
                            <p class="text">PocketMenu App. Kolay ve hızlı bir şekilde birkaç dakikada menünüzü oluşturup yayınlayabileceğiniz en iyi ücretsiz menü aracı.</p>
                        </div> <!-- footer about -->
                    </div>
                    <div class="col-lg-5 col-md-7 col-sm-7">
                     
                    </div>
                    <div class="col-lg-3 col-md-5 col-sm-5">
                        <div class="footer-contact mt-50 wow fadeIn" data-wow-duration="1s" data-wow-delay="0.8s">
                            <div class="footer-title">
                                <h4 class="title">İletişim</h4>
                            </div>
                            <ul class="contact">
                                <li>+905439115493</li>
                                <li>admin@pma.ist</li>
                                <li>www.pma.ist</li>
                                @*<li>123 Stree New York City , United <br> States Of America 750.</li>*@
                            </ul>
                        </div> <!-- footer contact -->
                    </div>
                </div> <!-- row -->
            </div> <!-- footer widget -->
            <div class="footer-copyright">
                <div class="row">
                    <div class="col-lg-12">
                        <div class="copyright d-sm-flex justify-content-between">
                            <div class="copyright-content">
                                <p class="text"><a href="" rel="nofollow">SlothDevelopment</a> @DateTime.Now.Year</p>
                            </div> <!-- copyright content -->
                        </div> <!-- copyright -->
                    </div>
                </div> <!-- row -->
            </div> <!-- footer copyright -->
        </div> <!-- container -->
        <div id="particles-2"></div>
    </footer>

    <!--====== FOOTER PART ENDS ======-->
    <!--====== BACK TOP TOP PART START ======-->

    <a href="#" class="back-to-top"><i class="lni lni-chevron-up"></i></a>

    <!--====== BACK TOP TOP PART ENDS ======-->
    <!--====== PART START ======-->
    <!--
        <section class="">
            <div class="container">
                <div class="row">
                    <div class="col-lg-"></div>
                </div>
            </div>
        </section>
    -->
    <!--====== PART ENDS ======-->
    <!--====== Jquery js ======-->
    <script src="~/Content/src/landing-assets/js/vendor/jquery-3.5.1-min.js"></script>
    <script src="~/Content/src/landing-assets/js/vendor/modernizr-3.7.1.min.js"></script>

    <!--====== Bootstrap js ======-->
    <script src="~/Content/src/landing-assets/js/popper.min.js"></script>
    <script src="~/Content/src/landing-assets/js/bootstrap-4.5.0.min.js"></script>

    <!--====== Plugins js ======-->
    <script src="~/Content/src/landing-assets/js/plugins.js"></script>

    <!--====== Counter Up js ======-->
    <script src="~/Content/src/landing-assets/js/waypoints.min.js"></script>
    <script src="~/Content/src/landing-assets/js/jquery.counterup.min.js"></script>



    <!--====== Scrolling Nav js ======-->
    <script src="~/Content/src/landing-assets/js/jquery.easing.min.js"></script>
    <script src="~/Content/src/landing-assets/js/scrolling-nav.js"></script>

    <!--====== wow js ======-->
    <script src="~/Content/src/landing-assets/js/wow.min.js"></script>

    <!--====== Particles js ======-->
    <script src="~/Content/src/landing-assets/js/particles.min.js"></script>

    <!--====== Main js ======-->
    <script src="~/Content/src/landing-assets/js/main.js"></script>

</body>

</html>
